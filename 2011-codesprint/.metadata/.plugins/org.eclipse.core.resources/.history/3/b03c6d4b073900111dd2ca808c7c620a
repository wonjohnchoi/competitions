def tokenize():
    while True:
        for token in input().split(' '):
            if token.strip() != '':
                yield token
tokens = tokenize()
INT = lambda : int(next(tokens))
FLOAT = lambda : float(next(tokens))
STR = lambda : next(tokens)

t = INT()
for _ in range(t):
    n, m = INT(), INT()
    print('%.2f'%(pow(n + 1) - pow(m + 1)))